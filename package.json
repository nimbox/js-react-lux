{
  "name": "@nimbox/js-react-lux",
  "version": "0.4.31",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./icons": "./dist/icons/index.js",
    "./figures": "./dist/figures/index.js",
    "./styles/elegant": "./dist/styles/elegant.js"
  },
  "typesVersions": {
    "*": {
      "icons": [
        "./dist/icons/index.d.ts"
      ],
      "figures": [
        "./dist/figures/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist/* && npm run build:icons && rollup -c",
    "build:watch": "rm -rf dist/* && npm run build:icons && rollup -cw",
    "build:icons": "rm -f src/icons/components/*.tsx && cp src/icons/sources/*.svg src/icons/outlines && svgr --config-file icons.svgr.config.js --icon --typescript --silent --out-dir src/icons/components src/icons/outlines",
    "build:style": "tailwind build src/styles/index.css -o src/styles/styles.css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6096 -s ./src",
    "build-storybook": "build-storybook -s ./src",
    "preversion": "npm install && npm run build",
    "postversion": "git push origin main --follow-tags && npm publish"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@nimbox"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-popper": "^2.3.0",
    "react-spring": "^9.5.2",
    "tinycolor2": "^1.4.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/cli": "^6.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.49",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/tinycolor2": "^1.4.3",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.8",
    "babel-plugin-named-exports-order": "^0.0.2",
    "classnames": "^2.3.1",
    "eslint-plugin-mdx": "^2.0.2",
    "eslint-plugin-react": "^7.30.1",
    "i18next": "^21.9.0",
    "i18next-http-backend": "^1.4.1",
    "lodash": "^4.17.21",
    "postcss": "^8.4.16",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.1",
    "react-i18next": "^11.18.4",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "rollup": "^2.78.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-typescript2": "^0.32.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:mdx/recommended"
    ],
    "settings": {
      "mdx/code-blocks": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

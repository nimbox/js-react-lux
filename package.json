{
  "name": "js-react-lux",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run build:style && npm run build:icons && react-scripts start",
    "build": "rm -rf dist/* && npm run build:style && npm run build:icons && cp src/styles/*.js dist/ && rollup -c",
    "build:watch": "rm -rf dist/* && npm run build:style && npm run build:icons && cp src/styles/*.js dist/ && rollup -cw",
    "build:style": "tailwind build src/styles/index.css -o src/styles/styles.css",
    "build:icons": "rm -f src/icons/*.tsx && svgr --icon --typescript --silent --out-dir src/icons src/icons",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "npm run build:style && npm run build:icons && start-storybook -s src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.4.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.18",
    "@svgr/cli": "^5.4.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@testing-library/user-event": "^10.1.0",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^5.1.5",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "i18next": "^19.4.4",
    "i18next-xhr-backend": "^3.2.2",
    "postcss-cli": "^7.1.1",
    "react-scripts": "3.4.1",
    "rollup": "^2.7.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "tailwindcss": "^1.3.5",
    "typescript": "~3.8.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

{
  "name": "@nimbox/js-react-lux",
  "version": "0.4.25",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./icons": "./dist/icons/index.js",
    "./figures": "./dist/figures/index.js",
    "./styles/elegant": "./dist/styles/elegant.js"
  },
  "typesVersions": {
    "*": {
      "icons": [
        "./dist/icons/index.d.ts"
      ],
      "figures": [
        "./dist/figures/index.d.ts"
      ]
    }
  },
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist/* && npm run build:icons && rollup -c",
    "build:watch": "rm -rf dist/* && npm run build:icons && rollup -cw",
    "build:icons": "rm -rf src/icons/components/* && cp src/icons/sources/*.svg src/icons/outlines && svgr --config-file icons.svgr.config.js --out-dir src/icons/components --typescript src/icons/outlines",
    "build:style": "tailwind build src/styles/index.css -o src/styles/styles.css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6096 -s ./src",
    "build-storybook": "build-storybook -s ./src",
    "preversion": "npm install && npm run build",
    "postversion": "git push origin main --follow-tags && npm publish"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@nimbox"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "react-dnd": "^14.0.5",
    "react-dnd-html5-backend": "^14.1.0",
    "react-popper": "^2.2.5",
    "react-spring": "^9.4.2",
    "tinycolor2": "^1.4.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.16",
    "@storybook/addon-essentials": "^6.4.16",
    "@storybook/addon-links": "^6.4.16",
    "@storybook/builder-webpack5": "^6.4.16",
    "@storybook/manager-webpack5": "^6.4.16",
    "@storybook/node-logger": "^6.4.16",
    "@storybook/preset-create-react-app": "^4",
    "@storybook/react": "^6.4.16",
    "@svgr/cli": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/tinycolor2": "^1.4.3",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.2",
    "classnames": "^2.3.1",
    "eslint-plugin-mdx": "^1.16.0",
    "eslint-plugin-react": "^7.28.0",
    "i18next": "^21.6.10",
    "i18next-http-backend": "^1.3.2",
    "lodash": "^4.17.21",
    "postcss": "^8.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.25.3",
    "react-i18next": "^11.15.3",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "rollup": "^2.66.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:mdx/recommended"
    ],
    "settings": {
      "mdx/code-blocks": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
